{
  "targets": {
    "release": {
      "outFile": "build/rle_assemblyscript.wasm",
      "textFile": "build/rle_assemblyscript.wat",
      "sourceMap": false,
      "optimize": true,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": true
    },
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "sourceMap": true,
      "debug": true
    }
  },
  "options": {
    "bindings": "esm",
    "exportRuntime": true
  }
}
